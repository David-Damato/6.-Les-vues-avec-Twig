{% extends 'base.html.twig' %}

{% block title %}
Product List
{% endblock %}

{% block content %}
    

        <div class="row row-cols-1 row-cols-md-3">
            {% for product in products %}
                <div class="col mb-4">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">product title</h5>
                            <p class="card-text">{{ product }}</p>
                        </div>
                    </div>
                </div>
            {% endfor %}
        </div>

{% endblock %}